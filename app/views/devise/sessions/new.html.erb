<div class="container">
  <div class="row spacing-signup">
    <div class="col-4 text-left">
      <div class="login-card">
        <h4 class="text-center">Welcome back to <strong class="highlight">DeciDesk</strong> </h4>
     
        <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
       <div class="form-inputs">
         <%= f.input :email,
                     required: false,
                     autofocus: true,
                     input_html: { autocomplete: "email" } %>
         <%= f.input :password,
                     required: false,
                     input_html: { autocomplete: "current-password" } %>
         <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
       </div>

        <div class="form-actions">
          <div class="btn-nav">
            <%= f.button :submit, "Log in" %>
          </div>
        </div>
        <% end %>

        <div class="mt-3">
          <p>Don't have an account yet ðŸ‘‰ <strong class="highlight"><%= render "devise/shared/links" %></strong></p>
        </div>
     
      </div>
    </div>

